  "routing_rules": {
    "vision_processing": {
      "triggers": ["image", "图片", "视觉", "看", "分析图", "photo", "picture"],
      "required_capabilities": ["vision"],
      "fallback_destination": "simple_chat"
    },
    "complex_reasoning": {
      "triggers": ["推理", "逻辑", "分析", "reasoning", "logic", "analyze", "think", "解决", "problem"],
      "required_capabilities": ["reasoning"],
      "fallback_destination": "simple_chat"
    },
    "code_generation": {
      "triggers": ["代码", "编程", "code", "program", "function", "class", "算法", "debug", "bug"],
      "required_capabilities": ["code_generation"],
      "fallback_destination": "simple_chat"
    },
    "chinese_conversation": {
      "triggers": ["中文", "chinese", "汉语", "普通话"],
      "required_capabilities": ["chinese"],
      "fallback_destination": "simple_chat"
    },
    "web_search": {
      "triggers": ["搜索", "search", "最新", "latest", "current", "news", "今天", "现在"],
      "required_capabilities": ["search", "web_search"],
      "fallback_destination": "simple_chat"
    },
    "mathematical_computation": {
      "triggers": ["数学", "计算", "math", "calculate", "equation", "formula", "solve"],
      "required_capabilities": ["mathematical_computation"],
      "fallback_destination": "simple_chat"
    },
    "structured_analysis": {
      "triggers": ["结构化", "格式", "extract", "parse", "json", "xml", "table"],
      "required_capabilities": ["structured_output"],
      "fallback_destination": "simple_chat"
    },
    "document_retrieval": {
      "triggers": ["文档", "知识", "rag", "retrieval", "langchain", "vector"],
      "required_capabilities": ["reasoning"],
      "fallback_destination": "simple_chat"
    },
    "agent_execution": {
      "triggers": ["任务", "步骤", "agent", "tool", "执行", "workflow"],
      "required_capabilities": ["agents", "tool_calling"],
      "fallback_destination": "simple_chat"
    },
    "creative_writing": {
      "triggers": ["创作", "写作", "故事", "creative", "write", "story", "poem"],
      "required_capabilities": ["creative_writing"],
      "fallback_destination": "simple_chat"
    },
    "simple_chat": {
      "triggers": ["聊天", "chat", "hello", "hi", "你好"],
      "required_capabilities": [],
      "fallback_destination": "simple_chat"
    }
  },
